generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Book {
  id          String   @id @default(cuid())
  title       String
  imageUrl    String
  status      String   @default("AVAILABLE") // AVAILABLE, RESERVED, TAKEN
  renterName  String?
  renterPhone String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
